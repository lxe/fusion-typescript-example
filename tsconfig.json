{
  "compilerOptions": {
    "outDir": "ts-output",
    "sourceMap": true,
    "jsx": "react",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "module": "es2015",
    "target": "es5",
    "typeRoots": [
      "./types",
      "./node_modules/@types"
    ],
    "baseUrl": "./",
    "paths": {
      "src/*": [
        "src/*"
      ],
      "*": [
        "types/*"
      ]
    }
  }
}